{
    "name": "Geekometer",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": ["*://ctftime.org/writeup/*"],
        "js": ["script.js"]
      }],
      "browser_action": {
        "default_title": "Geeks",
        "default_popup": "popups/popup.html"
      },
      "background": {
        "scripts": ["background.js"]
      },
      "permissions": ["tabs"]
}